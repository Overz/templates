{
  "name": "backend-default",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "start": "node build/index.js",
    "demo": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts --poll --transpile-only --ignore-watch node_modules",
    "build": "npm run clean && tsc -p tsconfig.prod.json --removeComments",
    "test": "jest",
    "watch": "jest --watchAll --no-cache",
    "style": "prettier --check 'src/**/*.ts'",
    "lint": "eslint --max-warnings=0",
    "coverage": "jest --coverage",
    "check": "npm run style && npm run lint && npm run coverage",
    "orm": "ts-node ./node_modules/typeorm/cli.js",
    "db:migrate": "npm run orm migration:run",
    "db:drop": "npm run orm schema:drop",
    "db:create": "npm run orm migration:create -- -n $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Overz/templates.git"
  },
  "author": "Christian Lima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Overz/templates/issues"
  },
  "homepage": "https://github.com/Overz/templates/blob/master/README.md",
  "dependencies": {
    "axios": "0.22.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "date-fns": "2.25.0",
    "env-var": "7.0.1",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-validator": "6.12.2",
    "jsonwebtoken": "8.5.1",
    "ms": "2.1.3",
    "nanoid": "3.1.29",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.38",
    "typescript": "4.4.3",
    "umzug": "2.3.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/ms": "0.7.31",
    "@types/nanoid": "3.0.0",
    "@types/supertest": "2.0.11",
    "@types/umzug": "2.3.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "10.0.0",
