{
  "name": "backend-default",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "start": "node build/index.js",
    "dev": "ts-node-dev src/index.ts --poll --transpile-only --ignore-watch node_modules",
    "build": "npm run clean && tsc -p tsconfig.prod.json --removeComments",
    "test": "jest",
    "watch": "jest --watchAll --no-cache",
    "style": "prettier --check 'src/**/*.ts'",
    "lint": "eslint --max-warnings=0",
    "coverage": "jest --coverage",
    "check": "npm run style && npm run lint && npm run coverage",
    "orm": "ts-node ./node_modules/typeorm/cli.js",
    "db:migrate": "npm run orm migration:run",
    "db:drop": "npm run orm schema:drop",
    "db:create": "npm run orm migration:create -- -n $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Overz/typescript-templates.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Overz/typescript-templates/issues"
  },
  "homepage": "https://github.com/Overz/typescript-templates#readme",
  "dependencies": {
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "date-fns": "2.21.3",
    "env-var": "7.0.1",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-validator": "6.11.1",
    "jsonwebtoken": "8.5.1",
    "ms": "2.1.3",
    "nanoid": "3.1.23",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.32",
    "typescript": "^4.2.4",
    "umzug": "2.3.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.1",
    "@types/ms": "0.7.31",
    "@types/nanoid": "2.1.0",
    "@types/supertest": "2.0.11",
    "@types/umzug": "2.3.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "dotenv": "10.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "sqlite3": "5.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-node-dev": "1.1.6"
  }
}
